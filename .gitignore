# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore Drupal's file directory
/web/sites/*/files/
/private/*
/tmp/*

# Ignore .htaccess
/web/.htaccess

# Ignore SimpleTest and Cypress multi-site environment
/web/sites/simpletest
/web/drupal-cypress-environment

# Ignore .env files as they are personal
/.env

# Ignore configuration files that may contain sensitive information
/web/sites/*/settings.local.php
/web/sites/*/settings.devdesktop.php
/web/sites/*/services*.yml
/web/sites/default/drushrc.php
.lando.local.default.yml
.lando.local.yml

# Ignore database dumps
*.sql.gz
*.sql

# Ignore Acquia Dev Desktop configs.
/web/sites/*.dd

# Ignore any pipeline base.
/scripts/deployment/base

# Ignore theme build as this gets compiled in pipelines.
web/themes/custom/europapolitik/dist/

# Ignore any node_modules directory
node_modules

# Ignore apple stuff
.DS_Store

# Ignore files generated by common IDEs
/.idea/
/.vscode/

# Locust
.env.locust
__pycache__
